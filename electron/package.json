{
  "name": "coinwallet",
  "private": true,
  "productName": "Coin Wallet",
  "executableName": "coin",
  "version": "6.5.0",
  "description": "Coin Wallet",
  "license": "MIT",
  "author": "Coin Wallet <support@coin.space> (https://coin.space/)",
  "homepage": "https://coin.space/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoinSpace/CoinSpace.git"
  },
  "type": "module",
  "main": "electron.js",
  "config": {
    "forge": "forge.config.js"
  },
  "scripts": {
    "start": "electron-forge start",
    "build": "node build.js",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint . --ext .js --report-unused-disable-directives",
    "clean": "rm -rf ./dist && rm -rf ./out"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-appx": "^7.4.0",
    "@electron-forge/maker-base": "^7.4.0",
    "@electron-forge/maker-dmg": "^7.4.0",
    "@electron-forge/maker-pkg": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/publisher-gcs": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "app-builder-lib": "^24.7.0",
    "ejs": "^3.1.9",
    "electron": "^31.0.2",
    "electron-packager-languages": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-config-coinspace": "^2.1.1"
  },
  "dependencies": {
    "@sentry/electron": "^4.11.0",
    "electron-log": "^5.0.0"
  },
  "eslintConfig": {
    "root": true,
    "ignorePatterns": [
      "/dist"
    ],
    "extends": [
      "eslint:recommended",
      "coinspace"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "requireConfigFile": false,
      "sourceType": "module"
    }
  }
}
