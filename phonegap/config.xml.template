<?xml version='1.0' encoding='utf-8'?>
<widget id="com.coinspace.app" android-versionCode="2185" ios-CFBundleVersion="1" version="2.20.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
  <name>Coin</name>
  <description>
    Coin application.
  </description>
  <author email="support@coin.space" href="https://www.coin.space">
    Coin Team
  </author>
  <content src="index.html" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <allow-navigation href="https://*/*" />
  <platform name="android">
    <allow-intent href="market:*" />
    <icon src="../images/android/icons/36.png" density="ldpi"/>
    <icon src="../images/android/icons/48.png" density="mdpi"/>
    <icon src="../images/android/icons/72.png" density="hdpi"/>
    <icon src="../images/android/icons/96.png" density="xhdpi"/>
    <splash src="../images/android/screens/ldpi-land.png" density="land-ldpi"/>
    <splash src="../images/android/screens/mdpi-land.png" density="land-mdpi"/>
    <splash src="../images/android/screens/hdpi-land.png" density="land-hdpi"/>
