<div class="widget-biometry-setup" class-_animated="animation" class-_open="isOpen">

  <div class="_content">

    <div class="_icon">
      {{>`${icon}`}}
    </div>

    <div class="_title">{{{header}}}</div>

    <p class="_description">{{description}}</p>

    <div class="csbtngroup">
      <div class="csbtn" on-click="confirm">{{buttonLabel}}</div>
    </div>

    <span class="_footer_link" on-click="back">{{translate("Skip")}}</span>
  </div>

</div>
