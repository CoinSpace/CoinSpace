<div class="confirm__responseIcon">
  {{>svg_error}}
</div>

<h3 class="confirm__responseTitle">{{translate("Transaction Failed")}}</h3>

<p class="confirm__responseMsg">{{translate(error, interpolations)}}</p>

<div class="confirm__inputs">
  <a class="csbtn--red" onclick="window.open('https://coinapp.zendesk.com/hc/en-us', '_blank').opener = null; return false;" href="">
    <span class="_text">{{translate("Support")}}</span>
  </a>
  <div class="csbtn--grey _cancel">{{translate("Close")}}</div>
</div>
