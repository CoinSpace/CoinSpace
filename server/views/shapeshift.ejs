<body>
  Loading...
<script>
  var accessToken = '<%= accessToken %>';
  var buildType = '<%= buildType %>';
  if (buildType === 'web') {
    window.localStorage.setItem('_cs_shapeshift_token', accessToken);
    window.close();
  } else if (buildType === 'phonegap') {
    window.location = 'coinspace://?action=shapeshift-login&accessToken=' + accessToken;
  } else if (buildType === 'electron') {
    window.location = 'coinspace://?action=shapeshift-login&accessToken=' + accessToken;
  }
</script>
</body>
</html>
